{
  "name": "eventify",
  "version": "1.0.0",
  "description": "University Club Event Management Platform",
  "main": "index.html",
  "scripts": {
    "build": "tailwindcss -i ./styles/tailwind.css -o ./styles/output.css --watch",
    "build:prod": "tailwindcss -i ./styles/tailwind.css -o ./styles/output.css --minify",
    "dev": "live-server --port=3000 --open=/",
    "deploy": "firebase deploy"
  },
  "keywords": [
    "events",
    "university",
    "firebase",
    "tailwind"
  ],
  "author": "Eventify Team",
  "license": "MIT",
  "devDependencies": {
    "firebase-tools": "^14.14.0",
    "live-server": "^1.2.2",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0"
  }
}
